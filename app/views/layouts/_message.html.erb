<% if notice %>  <p class="alert alert-dismissable alert-success fade in">    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>    <%= notice %>  </p><% end %><% if alert %>  <p class="alert alert-dismissable alert-danger fade in">    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>    <%= alert %>  </p><% end %>